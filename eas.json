{
  "cli": {
    "version": ">= 5.0.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": { "APP_VARIANT": "preview" }
    },
    "production": {
      "channel": "production",
      "env": { "APP_VARIANT": "production" }
    }
  },
  "submit": {
    "production": {}
  }
}
